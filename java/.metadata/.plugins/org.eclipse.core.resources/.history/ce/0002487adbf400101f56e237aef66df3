// *** 읽기 전용
// 1. 생성자 > 초기화
// 2. Getter 구현O, Setter 구현X
class UerClass {
	
	private String id;
	private String name;
	private int age;
	
	public UerClass(String id, String name, int age) {
		this.id = id;
		this.name = name;
		this.age = age;
	}
	
	public String getId() {
		return id;
	}
	public String getName() {
		return name;
	}
	public int getAge() {
		return age;
	}
};

@Override
public String toString() {
	return "UerClass [id=" + id + ", name=" + name + ", age=" + age + "]";
};